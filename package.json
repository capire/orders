{
  "name": "@capire/orders",
  "version": "2.0.1",
  "description": "A generic reuse service for managing orders.",
  "repository": "https://github.com/capire/orders",
  "dependencies": {
    "@cap-js/hana": ">=1",
    "@capire/common": "*",
    "@sap/cds": ">=5",
    "@sap/xssec": "^4",
    "@sap/cds-mtxs": "git+https://github.tools.sap/cap/cds-mtxs.git",
    "@sap-cloud-sdk/connectivity": "^4",
    "@sap-cloud-sdk/resilience": "^4",
    "@sap-cloud-sdk/http-client": "^4"
  },
  "cds": {
    "requires": {
      "messaging": true,
      "fiori": {
        "preview": true
      },
      "cds.xt.DeploymentService": false,
      "cds.xt.RemoteExtensibilityService": {
        "kind": "rest",
        "model": "@sap/cds-mtxs/srv/extensibility-service",
        "[development]": {
          "credentials": {
            "url": "http://localhost:4005"
          }
        },
        "[production]": {
          "credentials": {
            "destination": "mtx-api"
          }
        }
      },
      "multitenancy": true,
      "extensibility": true,
      "[development]": {
        "db": {
          "impl": "@cap-js/sqlite",
          "credentials": {
            "url": "../../../db.sqlite"
          },
          "kind": "sqlite"
        }
      }
    },
    "appid": true
  },
  "scripts": {
    "start": "cds-serve"
  }
}